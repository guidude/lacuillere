<%= form_for [@restaurant, @review] do |f| %>


  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </div>
  <div class="field">
    <%= f.label :rating %><br>
    <%= f.text_area :rating %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
